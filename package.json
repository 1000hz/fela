{
  "private": true,
  "version": "4.3.5",
  "description": "Universal, Dynamic & High-Performance Styling in JavaScript",
  "repository": "https://github.com/rofrischmann/fela/",
  "author": "Robin Frischmann",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "./scripts/build.sh",
    "clean": "lerna clean",
    "docs": "gitbook install && gitbook build && gh-pages -d _book",
    "check": "npm run lint && npm run test:coverage && npm run flow",
    "format": "prettier-eslint packages/**/*.js --write",
    "flow": "flow",
    "lint": "eslint packages/*/src/**/*.js",
    "publish": "git pull --rebase && npm run build && lerna publish",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:coverage": "cross-env BABEL_ENV=commonjs jest --coverage"
  },
  "jest": {
    "rootDir": "packages",
    "testPathIgnorePatterns": [
      "/mocks/"
    ]
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-inferno": "^1.9.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "cross-env": "^1.0.8",
    "css-in-js-utils": "^1.0.3",
    "cssbeautify": "^0.3.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.13.4",
    "express-http-proxy": "^0.6.0",
    "flow-bin": "^0.38.0",
    "fs-extra": "^0.30.0",
    "gh-pages": "^0.11.0",
    "gitbook-cli": "^2.3.0",
    "gzip-js": "^0.3.2",
    "gzip-size": "^3.0.0",
    "inferno": "^1.0.1",
    "inferno-component": "^1.0.1",
    "inferno-create-element": "^1.0.1",
    "inline-style-prefixer": "^3.0.1",
    "jest": "^19.0.2",
    "lerna": "^2.0.0-rc.4",
    "preact": "^7.0.0",
    "prettier-eslint": "^4.2.1",
    "prettier-eslint-cli": "^3.1.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-native": "^0.42.0",
    "rollup": "0.26.3",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-commonjs": "2.2.1",
    "rollup-plugin-node-resolve": "1.5.0",
    "rollup-plugin-uglify": "0.3.1"
  }
}
